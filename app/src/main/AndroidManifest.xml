<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mvcdemo.palmspeed.com.mvpdemoapp">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:fullBackupContent="false">

        <activity android:name=".AddFeature.AddInput.AddActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".About.View.AboutActivity" />
        <activity android:name=".AddFeature.AddResult.AddResultActivity" />

        <activity android:name=".Error.ErrorActivity" />
    </application>

</manifest>